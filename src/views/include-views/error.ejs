<div class="row w-75 mt-3">
    <div class="col">
        <% if(errors.length > 0) {%>
            <% errors.forEach((e) => { %>
                <div class="alert alert-danger text-center">
                    <%= e %> <br>
                </div>
            <% }) %>
        <% } %>
    </div>
</div>