<div class="row w-75 mt-3 mx-auto">
    <div class="col">
        <% if(success.length > 0) {%>
            <% success.forEach((e) => { %>
                <div class="alert alert-success text-center">
                    <%= e %> <br>
                </div>
            <% }) %>
        <% } %>
    </div>
</div>